          </ul>
        </div>
        <div class="col-md-10">
          <table class="table table-bordered">
            <thead class="text-center ">
              <tr>
                <th colspan="6">
                  <h2>Create Blog</h2>
                </th>
              </tr>
              <tr>
                <th>TITLE</th>
                <th>Posted on</th>
                <th>Action</th>
              </tr>
            </thead>
            <tbody class="text-center">
              <!-- loop for multile data --> 
              <% data.forEach((item)=>{ %>
                <tr>
                  <td>
                    <%= item.catName %>
                  </td>
                  <td>
                    <%= item.createdAt.toLocaleDateString() %>
                  </td>
                  <td>
                    <a href="/admin/editCat/<%= item._id %>"><button class="btn bg-success">Edit</button></a>
                    <a href="/admin/deleteCat/<%= item._id %>"><button class="btn bg-danger">Delete</button></a>
                    <a href="/admin/viewCat/<%= item._id %>"><button class="btn" style="background:#049eb7;">View</button></a>
                  </td>
                </tr>
              <% }) %>    
                
                

            </tbody>

          </table>
          <a href="/admin/createCategory"><button class="btn" style="margin-left: 72%; background: #049eb7;">Add
              Categrious</button></a>
