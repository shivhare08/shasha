<%- include('../header.ejs')%>
<section class="readmore">
    <div class="container"style="margin-top: 120px; font-family: cursive;">
      <div class="row">
        <div class="col-md-7">
          <h2 style="color:#60748b"><%= result.title %></h2>
          <h3 class="text-center"><%= result.name %></h3>
            <p class="text-center"><%= result.description %></p>
            <img src="/images/web3.jpg" class="img-fluid" style="width:100%;">
            <p class="mt-4"><%= result.description %></p>
        </div>
        <div class="col-md-5">
          <div>
            <div class="bg-dark"><h4 class="text-center" style="color:whitesmoke;">Recent Blog</h4></div>
            
            <div class="row mt-4">
              <div class="col-md-6">
                <img src="/images/sharepoint.jpg" class="img-fluid">
              </div>
              <div class="col-md-6">
                <h4>Blog Title</h4>
                <p><span style="color:#60748b">and what doesn’t when it comes to creating them. To help web creators develop a sense of what makes a story</span></p><hr>
              </div>
            </div> <!-- row end -->
  
            <div class="row">
              <div class="col-md-6">
                <img src="/images/web6.jpg" class="img-fluid">
              </div>
              <div class="col-md-6">
                <h4>Blog Title</h4>
              <p><span style="color:#60748b"> To help web creators develop a sense of what makes a story great, we’re kicking off a series to share tips</span></p><hr>
              </div>
            </div> <!-- row end -->
  
            
  
            <div class="row">
              <div class="col-md-6">
                <img src="/images/node.png" class="img-fluid">
              </div>
              <div class="col-md-6">
                <h4>Blog Title</h4>
              <p><span style="color:#60748b">read it as Web Story on your phone, or scroll on if you prefer reading when it comes to creating them. </span></p><hr>
              </div>
            </div> <!-- row end -->
            <div class="bg-dark">
              <h4 class="text-center mt-4" style="color:whitesmoke;">Categrious</h4></div>
              <% categoryData.forEach((item)=>{  %>
                <a href="/admin/viewCat/<%= item._id %>" class="nav-link" style="padding-left: 3%;" ><%= item.catName %></a><hr>
              
              <% })%>
                
              
            </div>
          </div>
        </div>
      </div> <!-- row end -->
    </div> <!-- container end -->
  </section>
<%- include('../footer.ejs')%>
